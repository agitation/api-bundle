{
    "name": "agitation/api-bundle",
    "type": "symfony-bundle",
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.6",
        "symfony/symfony": "2.8.*",
        "agitation/base-bundle": "dev-master",
        "agitation/validation-bundle": "^1.0",
        "agitation/logging-bundle": "dev-master"
    },
    "suggest": {
        "agitation/user-bundle": "Required to check for user capabilities. Without it, all calls requiring a capability will fail."
    },
    "autoload": {
        "psr-4": {
            "Agit\\ApiBundle\\": "lib/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "AGITsol GmbH",
            "homepage": "http://www.agitsol.com/"
        }
    ]
}
